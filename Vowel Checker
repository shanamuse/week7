#include <stdio.h>

int is_vowel(char c) {
    if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
        c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
        return 1;
    } else {
        return 0;
    }
}

int main() {
    char input;
    int result;

    printf("Enter:\n");
    scanf(" %c", &input);  
    
    result = is_vowel(input);

    printf("Is it a vowel: %d\n", result);

    return 0;
}
